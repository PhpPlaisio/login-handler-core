{
  "name": "plaisio/login-handler-core",
  "description": "PhpPlaisio: Core Login Handler",
  "keywords": [
    "PhpPlaisio",
    "Login Handler",
    "Core"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/company-resolver": "^2.2.0",
    "plaisio/kernel": "^3.3.1",
    "plaisio/login-handler": "^1.4.0",
    "plaisio/login-requirement": "^1.3.0",
    "plaisio/request": "^2.0.0",
    "plaisio/session": "^5.1.0",
    "setbased/php-stratum-mysql": "^7.2.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.25",
    "plaisio/console-kernel": "^1.2.0",
    "plaisio/request-core": "^2.0.0",
    "setbased/php-stratum": "^6.5.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Login\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Login\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
