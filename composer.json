{
  "name": "plaisio/login-handler-core",
  "description": "PhpPlaisio: Core Login Handler",
  "keywords": [
    "PhpPlaisio",
    "Login Handler",
    "Core"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/kernel": "^1.0",
    "plaisio/login-handler": "^1.0",
    "plaisio/login-requirement": "^1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/request-core": "^0.10.0",
    "setbased/php-stratum": "^5.4",
    "setbased/php-stratum-mysql": "^4.4"

  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Login\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Login\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
